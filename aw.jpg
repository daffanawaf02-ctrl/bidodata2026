<script>
	var bodytext = document.querySelector('.paragraph').innerHTML;
	var tag = 'kisah sukses tukang bakso,tukang bakso,megawati sindir tukang bakso,kampung tukang bakso,desa bubakan wonogiri'.split(",");
	tag = tag.sort((a,b) => b.length - a.length);

	var regexBacajuga= new RegExp('<p><strong>Baca Juga:.*?<\/strong><\/p>', "g");
	var baca_juga = bodytext.match(regexBacajuga)
	for(let i=0 ; i<tag.length;i++){
		tag_url = window.location.origin+"/tag/"+tag[i].replace(" ", "-").toLowerCase()
		var regexStr= new RegExp('(?!([^<]+)?>)(?!<figcaption[^>]*?>)('+tag[i]+')(?![^<]*?<\/figcaption>)', 'i');

		var regexTags= new RegExp('('+tag[i]+')(?![^<]*>)', 'i'); // exclude match inside < >
		var realText = bodytext.match(regexTags);
		if (realText !== null) {
			if (realText.toString().includes(",")) {
				realText = realText.toString().split(',')[0];
			}
		}

		bodytext = bodytext.replace(regexStr,'<a href='+tag_url+' class="linkparagraph">'+realText+'</a>');
	}

	var baca = bodytext.match(regexBacajuga)
	if (baca) {
		for(let i=0 ; i<baca.length;i++){
			bodytext = bodytext.replace(baca[i],baca_juga[i]);
		}
	}

	var categoryName = $("meta[name='content_category']").attr("content").toLowerCase();
	if (categoryName == 'foto') {
		var regexImg = new RegExp('<figure\\b\\s*"?.*?"?\\s*>[\\S\\s]*?<\\/figure>', 'gi');
		var images = bodytext.match(regexImg);
		for(let i=0 ; i<images.length;i++){
			bodytext = bodytext.replace(images[i],'');
		}
	}
	
	var paragraph = document.getElementById('paragraph');
	paragraph.innerHTML = bodytext;

	function getOS() {
		var userAgent = window.navigator.userAgent,
			platform = window.navigator.platform,
			macosPlatforms = ['Macintosh', 'MacIntel', 'MacPPC', 'Mac68K'],
			windowsPlatforms = ['Win32', 'Win64', 'Windows', 'WinCE'],
			iosPlatforms = ['iPhone', 'iPad', 'iPod'],
			os = null;

		if (macosPlatforms.indexOf(platform) !== -1) {
			os = 'Mac OS';
		} else if (iosPlatforms.indexOf(platform) !== -1) {
			os = 'iOS';
		} else if (windowsPlatforms.indexOf(platform) !== -1) {
			os = 'Windows';
		} else if (/Android/.test(userAgent)) {
			os = 'Android';
		} else if (!os && /Linux/.test(platform)) {
			os = 'Linux';
		}

		return os;
	}

	var os_type = getOS();
	$('.deviceInfo').val(os_type);
	if (os_type == 'iOS') {
		$('.widgetSurvey').css('display', 'none');
	} else {
		$('.widgetSurvey').css('display', 'block');
	}
	
</script>

